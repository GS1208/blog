<h1>All Articles </h1>
<%= link_to "New Articles",new_article_path%>
<table border="1">
<tr>
<th> Title </th>
<th> Show </th>
<th> Edit </th>
<th> Delete </th>
</tr>
<% @articles.each do |a| %>

<tr>
<td><%= a.title%></td>
<td><%= link_to "More", article_path(a.id)%></td>
<td><%= link_to "Edit_Article", edit_article_path(a.id)%></td>
<td><%= link_to "Delete_Article", article_path(a.id), method: :delete, data: { confirm: 'Are you sure..?'}%></td>
</tr>
<%end%>
</table>
